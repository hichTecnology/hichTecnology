{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "name": "category"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "works": {
      "via": "categories",
      "collection": "work"
    },
    "customers": {
      "via": "categories",
      "collection": "customer"
    }
  }
}
